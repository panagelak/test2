<launch>

  <node name="pcl_deserializer" pkg="pcl_serializer" type="pcl_deserializer"  output = "screen">
    <!-- <remap from="input_pointcloud" to="/zed2/zed_node/point_cloud/cloud_registered"/> -->
    <!-- <remap from="input_pcl_transfer" to="/transfer_pcl"/> -->
    <remap from="image_out" to="/zed2/image_compressed"/>
    <remap from="depth_out" to="/zed2/depth_registered"/>
    <!-- <remap from="camera_info_in" to="/zed2/zed_node/depth/camera_info"/> -->
    <remap from="camera_info_out" to="/zed2/camera_info"/>
  </node>

  <node pkg="nodelet" type="nodelet" name="nodelet_manager" args="manager" />

  <node pkg="nodelet" type="nodelet" name="nodelet1"
        args="load depth_image_proc/point_cloud_xyz nodelet_manager">
    <remap from="camera_info" to="/zed2/camera_info"/>
    <remap from="image_rect" to="/zed2/depth_registered"/>
    <remap from="points" to="/zed2/point_cloud"/>
    
  </node>
 


</launch>

